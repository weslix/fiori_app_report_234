<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.f" xmlns:fb="sap.ui.comp.filterbar" xmlns:svm="sap.ui.comp.smartvariants" controllerName="br.com.gestao.fioriappreport234.controller.i18n" displayBlock="true">
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
                        
                    <sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" layout="ResponsiveGridLayout" id="form0">
                        
                        <sap.ui.layout.form:content>
                        
                            <core:Title text="{i18n>titleClientes}" id="title0"/>
                        
                            <Label text="{i18n>lblCliente}" id="label0"/>
                        
                            <Input width="100%" id="cliente" placeholder="{i18n>placeholderCliente}"/>
                        
                            <Label text="{i18n>lblEnderecoCliente}" id="label1"/>
                        
                            <Input width="100%" id="endereco" placeholder="{i18n>placeholderEnderecoCliente}"/>
                        
                            <Label text="{i18n>lblCidadeCliente}" width="100%" id="label2"/>
                        
                            <Input xmlns="sap.m" id="cidade" placeholder="{i18n>placeholderCidadeCliente}"/>
                        
                            <Label text="{i18n>lblEstadoCliente}" width="100%" id="label3"/>
                        
                            <Input xmlns="sap.m" id="estado" placeholder="{i18n>placeholderEstadoCliente}"/>
                        
                            <Button xmlns="sap.m" text="{i18n>btnEnviar}" press="enviarCadastro" id="button0" icon="sap-icon://action"/></sap.ui.layout.form:content>
                        
                    </sap.ui.layout.form:SimpleForm>
                        
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>